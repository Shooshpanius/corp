
<%= javascript_include_tag "main" %>
<%= javascript_include_tag "pbxadmin/international_access" %>


<div class="box">

  <div class="box-header">
    <h3 class="box-title">Доступ к международным вызовам</h3>
    <button type="button"  class="btn btn-success pull-right" data-toggle="modal" data-target="#addInternationalAccessModal"><i class="fa fa-plus"></i></button>
  </div>




  <div class="box-body">

    <table class="table table-bordered table-condensed tablesorter" id="tbl_adresses">
      <thead>

      <tr>
        <th data-placeholder="" class="filter-match col-md-1">id</th>
        <th data-placeholder="" class="filter-match col-md-2">Внутр. номер</th>
        <th data-placeholder="" class="filter-match col-md-9">Сотрудник</th>
        <th data-placeholder="" class="filter-match col-md-1"></th>

      </tr>

      </thead>

      <tbody>
      <% @form_data[:ia].each.with_index do |ia, index| %>
          <tr id="r_<%= ia.id %>">

            <td><%= ia.id %></td>
            <td><%= ia.number %></td>
            <td>


            </td>
          </tr>
      <% end %>
      </tbody>
    </table>

  </div>

</div>




</div><!-- /.modal -->